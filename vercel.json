{
  "version": 2,
  "builds": [
    {
      "src": "addSubscriber.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/addSubscriber",
      "methods": ["POST"],
      "dest": "addSubscriber.js"
    },
    {
      "src": "/(.*)",
      "dest": "addSubscriber.js"
    }
  ]
}
